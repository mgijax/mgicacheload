#!/bin/csh -f

#
# Usage:  inferredfrom.gorat
#
# History
#
# 03/04/2010	lec
#	- TR 10011; script for running the GOC load
#

cd `dirname $0` && source ./Configuration

setenv LOG	${MGICACHELOGDIR}/`basename $0`.log
rm -rf $LOG
touch $LOG

date | tee -a ${LOG}

${PYTHON} ./inferredfrom.py -S${MGD_DBSERVER} -D${MGD_DBNAME} -U${MGD_DBUSER} -P${MGD_DBPASSWORDFILE} -B"GOC" |& tee -a ${LOG}

date | tee -a ${LOG}
